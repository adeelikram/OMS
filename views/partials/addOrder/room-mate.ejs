<div class="col-xl-12">
    <div class="submit-field">
        <hr>
    </div>
</div>
<div class="col-xl-12">
    <div class="submit-field">
        <h3><b>Roomate</b></h3>
    </div>
</div>
<div class="col-xl-4">
    <div class="submit-field">
        <h5>Number of units</h5>
        <input <%- disabled ? (' disabled') : '' %> type="number"
            name="roomMate_units" class="with-border"
            placeholder="Number of units"
            value="<% if (editing) { %><%= order.roomMate.units.bought %><% } %>">
    </div>
</div>
<div class="col-xl-4">
    <div class="submit-field">
        <h5>Period(months)</h5>
        <input <%- disabled ? (' disabled') : '' %> type="number"
            name="roomMate_period" class="with-border"
            placeholder="Period(months), sold"
            value="<% if (editing) { %><%= order.roomMate.period %><% } %>">
    </div>
</div>
<div class="col-xl-4">
    <div class="submit-field">
        <h5>Installation</h5>
        <select name="roomMate_installation"
            id="roomMate_installation"
            class="browser-default custom-select "
            data-live-search="true">
            <% if (editing) { %>
                <% if (order.roomMate.installation) { %>
                    <option value="true" selected>Yes</option>
                    <option value="false">No</option>      
                <% } else { %>
                    <% if (order.roomMate.installation === undefined) { %>
                            <option value="" selected>Select</option>
                            <option value="true">Yes</option>
                            <option value="false">No</option>
                    <% } else { %>
                            <option value="true">Yes</option>
                            <option value="false" selected>No</option>
                    <% } %>
                <% } %>
            <% } else { %>
                <option value="" selected>Select</option>
                <option value="true">Yes</option>
                <option value="false">No</option>
            <% } %>
        </select>
    </div>
</div>

<div class="col-xl-4">
    <div class="submit-field">
        <h5>Type</h5>
        <select name="roomMate_type"
            id="roomMate_type"
            class="browser-default custom-select "
            data-live-search="true">
            <% if (editing) { %>
                <option value="rent" <%- order.roomMate.type === 'rent' ? 'selected' : '' %>>Rent</option>
                <option value="sold" <%- order.roomMate.type === 'sold' ? 'selected' : '' %>>Sold</option>
            <% } else { %>
                <option value="rent" selected>Rent</option>
                <option value="sold">Sold</option>
            <% } %>
        </select>
    </div>
</div>
