{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "prefer-promise-reject-errors": 1,
        "eqeqeq": 1,
        "camelcase": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": 0,
        "react/destructuring-assignment": 1,
        "import/no-unresolved": 0,
        "prettier/prettier": [
            "warn",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 80,
                "tabWidth": 4,
                "endOfLine": "auto"
            }
        ]
    }
}